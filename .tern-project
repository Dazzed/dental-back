{
  "libs": [
    "chai",
    "ecma5",
    "ecma6"
  ],
  "loadEagerly": [
    "src/**/*.js",
    "api/**/*.js"
  ],
  "plugins": {
    "node": {},
    "es_modules": {},
    "modules": {},
    "node_resolve": {},
    "complete_strings": {
      "maxLength": 15
    },
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
